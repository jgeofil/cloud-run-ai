project_id                 = "my-gcp-project"
region                     = "us-central1"
environment                = "dev"
cloud_run_image            = "us-central1-docker.pkg.dev/my-gcp-project/n8n/n8n:latest"
cloud_run_service_account  = "n8n-runner@my-gcp-project.iam.gserviceaccount.com"
cloud_run_max_instances    = 5
cloud_run_env = {
  N8N_BASIC_AUTH_USER     = "admin"
  N8N_BASIC_AUTH_PASSWORD = "change-me"
}
invoker_members = [
  "allUsers"
]
function_bucket      = "my-gcp-project-n8n-functions"
function_entry_point = "handle"
pubsub_topic         = "projects/my-gcp-project/topics/n8n-hook"
